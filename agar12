    <script>
        function scrollToSection(sectionId) {
            document.getElementById(sectionId).scrollIntoView({
                behavior: 'smooth'
            });
        }

        function showContactForm() {
            scrollToSection('contacto');
            setTimeout(() => {
                document.getElementById('nombre').focus();
            }, 800);
        }

        // üîß PASO 1: Reemplaza "TU_CLAVE_PUBLICA_AQUI" con tu Public Key de EmailJS
        emailjs.init("czPyYZjx9l6343KxB");

        function handleSubmit(event) {
            event.preventDefault();
            
            const form = event.target;
            const submitButton = form.querySelector('button[type="submit"]');
            
            // Mostrar estado de carga
            submitButton.textContent = 'Enviando...';
            submitButton.disabled = true;
            
            // Preparar los datos del formulario
            const formData = {
                nombre: form.nombre.value,
                email: form.email.value,
                institucion: form.institucion.value || 'No especificada',
                telefono: form.telefono.value || 'No especificado',
                aplicacion: form.aplicacion.value || 'No especificada',
                mensaje: form.mensaje.value || 'Sin mensaje adicional'
            };
            
            // Mostrar datos en consola para pruebas (puedes quitar esto despu√©s)
            console.log('üìß Datos para enviar a barriosmorales360@gmail.com:', formData);
            
            // Preparar par√°metros para EmailJS
            const templateParams = {
                to_email: 'barriosmorales360@gmail.com',
                from_name: formData.nombre,
                from_email: formData.email,
                institucion: formData.institucion,
                telefono: formData.telefono,
                aplicacion: formData.aplicacion,
                message: formData.mensaje,
                reply_to: formData.email
            };
            
            // üîß PASO 2: Reemplaza "TU_SERVICE_ID" y "TU_TEMPLATE_ID" con tus IDs reales de EmailJS
            emailjs.send('service_sp5xmiy', 'template_1crpog8', templateParams)
                .then(function(response) {
                    console.log('‚úÖ Email enviado exitosamente:', response);
                    
                    // Mostrar mensaje de √©xito
                    document.getElementById('contactForm').style.display = 'none';
                    document.getElementById('successMessage').classList.remove('hidden');
                    
                    // Resetear formulario despu√©s de 5 segundos
                    setTimeout(() => {
                        form.reset();
                        document.getElementById('contactForm').style.display = 'block';
                        document.getElementById('successMessage').classList.add('hidden');
                        submitButton.textContent = 'Enviar Consulta';
                        submitButton.disabled = false;
                    }, 5000);
                    
                }, function(error) {
                    console.error('‚ùå Error al enviar email:', error);
                    
                    // Mostrar error al usuario
                    const errorDiv = document.createElement('div');
                    errorDiv.className = 'bg-red-50 border border-red-200 p-4 rounded-lg mt-4';
                    errorDiv.innerHTML = `
                        <div class="flex items-center space-x-3">
                            <span class="text-red-500 text-xl">‚ö†Ô∏è</span>
                            <div>
                                <h4 class="font-semibold text-red-800">Error al enviar mensaje</h4>
                                <p class="text-red-700 text-sm">Por favor, contacte directamente a: barriosmorales360@gmail.com</p>
                            </div>
                        </div>
                    `;
                    
                    form.parentNode.appendChild(errorDiv);
                    
                    // Quitar mensaje de error despu√©s de 8 segundos
                    setTimeout(() => {
                        errorDiv.remove();
                    }, 8000);
                    
                    submitButton.textContent = 'Enviar Consulta';
                    submitButton.disabled = false;
                });
        }

        // Smooth scrolling para navegaci√≥n
        document.querySelectorAll('a[href^="#"]').forEach(anchor => {
            anchor.addEventListener('click', function (e) {
                e.preventDefault();
                const target = document.querySelector(this.getAttribute('href'));
                if (target) {
                    target.scrollIntoView({
                        behavior: 'smooth'
                    });
                }
            });
        });
    </script>
